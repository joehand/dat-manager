DAT_DIR=/Users/karissa/dev/dats/dat-manager
LOG_DIR=/Users/karissa/dev/dats/dat-manager/logs

NAME="$1"
USER="$2"
DAT_PATH=$DAT_DIR/$NAME
LOG_PATH=$LOG_DIR/$NAME.log

if [ \! -e $DAT_PATH ]; then
  mkdir $DAT_PATH
fi

dat init --path=$DAT_PATH --no-prompt
psy start -n $NAME --cwd=$DAT_PATH -l $LOG_PATH -- dat serve
# psy start -n $NAME --cwd=$DAT_PATH-l $LOG_PATH -- taco-nginx --name $NAME dat serve 
